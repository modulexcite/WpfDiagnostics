<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>WpfDiagnostics</id>
    <version>1.0.0.0</version>
    <title>WpfDiagnostics</title>
    <authors>Tiesmaster</authors>
    <owners>Tiesmaster</owners>
    <licenseUrl>https://github.com/tiesmaster/WpfDiagnostics/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/tiesmaster/WpfDiagnostics</projectUrl>
    <iconUrl>http://ICON_URL_HERE_OR_DELETE_THIS_LINE</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A set of roslyn diagnostics geared towards WPF clean code.</description>
    <copyright>Copyright</copyright>
    <tags>wpf,diagnostics,analyzers,codefixers,roslyn</tags>
  </metadata>
  <!-- The convention for analyzers is to put language agnostic dlls in analyzers\dotnet and language specific analyzers in either analyzers\dotnet\cs or analyzers\dotnet\vb -->
  <files>
    <file src="*.dll" target="analyzers\dotnet\cs" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*;**\System.Composition.*" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>
